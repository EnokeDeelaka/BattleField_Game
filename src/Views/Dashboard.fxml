<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DashboardController">
   <ImageView fitHeight="400.0" fitWidth="600.0">
      <Image url="@../Images/BattleField.jpg" />
   </ImageView>
   <Text fill="WHITE" layoutX="116.0" layoutY="152.0" stroke="BLACK" strokeType="OUTSIDE" strokeWidth="0.0" text="BattleField">
      <font>
         <Font name="System Bold" size="73.0" />
      </font>
   </Text>
   <Button fx:id="Close" layoutX="574.0" layoutY="2.0" mnemonicParsing="false" onAction="#CloseAction" text="X" />
   <Button fx:id="Maximize" layoutX="531.0" layoutY="2.0" mnemonicParsing="false" onAction="#MaximizeAction" prefHeight="25.0" prefWidth="43.0" text="[]" />
   <Button fx:id="Minimize" layoutX="495.0" layoutY="2.0" mnemonicParsing="false" onAction="#MinimizeAction" prefHeight="25.0" prefWidth="36.0" text="-" />
   <Button fx:id="BtnStartGame" layoutX="515.0" layoutY="361.0" mnemonicParsing="false" onAction="#StartGameAction" prefHeight="25.0" prefWidth="80.0" text="Start Game">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
</AnchorPane>
